{
  "compilerOptions": {
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "target": "ES2022",
    "sourceMap": true,
    "outDir": "dist",
    "noImplicitAny": true,
    "noUncheckedIndexedAccess": true
  },
  "include": [
    "src"
  ]
}
